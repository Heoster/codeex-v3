{
  "name": "codeex-ai",
  "version": "2.0.0",
  "description": "Multi-provider AI assistant with Groq + Google Gemini, smart routing, web search, and enhanced mobile experience",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "node scripts/production-build.js",
    "build:next": "cross-env SKIP_ENV_VALIDATION=true ESLINT_NO_DEV_ERRORS=true next build",
    "build:win": "set SKIP_ENV_VALIDATION=true&& set ESLINT_NO_DEV_ERRORS=true&& next build",
    "build:netlify": "node scripts/netlify-build.js",
    "build:safe": "cross-env NODE_ENV=production SKIP_ENV_VALIDATION=true next build",
    "verify:deployment": "node scripts/verify-deployment.js",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run typecheck && npm run lint",
    "prebuild": "npm run validate || echo 'Validation warnings (continuing build)'"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.7",
